{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DF-Orgchart"
		},
		"Ls_MisumiRawData_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Ls_MisumiRawData'"
		},
		"Ls_MongoDb_Orgchart_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Ls_MongoDb_Orgchart'"
		},
		"Ls_SqlDB_Organizationchart_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Ls_SqlDB_Organizationchart'"
		},
		"Ls_MongoDb_Orgchart_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "orgchart"
		},
		"Ls_SqlDB_Organizationchart_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlserver-a.database.windows.net"
		},
		"Ls_SqlDB_Organizationchart_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sqldb_orgchart"
		},
		"Ls_SqlDB_Organizationchart_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Monish"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DF-Orgchart_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Ls_MisumiRawData_connectionString": {
						"value": "[parameters('Ls_MisumiRawData_connectionString')]"
					},
					"Ls_MongoDb_Orgchart_connectionString": {
						"value": "[parameters('Ls_MongoDb_Orgchart_connectionString')]"
					},
					"Ls_SqlDB_Organizationchart_password": {
						"value": "[parameters('Ls_SqlDB_Organizationchart_password')]"
					},
					"Ls_MongoDb_Orgchart_properties_typeProperties_database": {
						"value": "[parameters('Ls_MongoDb_Orgchart_properties_typeProperties_database')]"
					},
					"Ls_SqlDB_Organizationchart_properties_typeProperties_server": {
						"value": "[parameters('Ls_SqlDB_Organizationchart_properties_typeProperties_server')]"
					},
					"Ls_SqlDB_Organizationchart_properties_typeProperties_database": {
						"value": "[parameters('Ls_SqlDB_Organizationchart_properties_typeProperties_database')]"
					},
					"Ls_SqlDB_Organizationchart_properties_typeProperties_userName": {
						"value": "[parameters('Ls_SqlDB_Organizationchart_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "DF-Orgchart_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'DF-Orgchart_ArmTemplate_0')]"
			]
		}
	]
}