									"path": "$['department']"
								},
								"sink": {
									"name": "department",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['reportsTo']"
								},
								"sink": {
									"name": "reportsTo",
									"type": "String"
								}
							}
						],
						"mapComplexValuesToString": true
					}
				},
				"inputs": [
					{
						"referenceName": "Ds_Mongo_Users",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Ds_Sql_Users",
						"type": "DatasetReference"
					}
				]
			},
